
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />

<form method="post">
    <label>Başlık</label>
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı yazınız" /><br />

    <label>Alt Başlık</label>
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlığı yazınız" /><br />

    <label>Tarih</label>
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihi" /><br />

    <label>Açıklama</label>
    <textarea name="Aciklama" rows="5" class="form-control" placeholder="Açıklamanızı Giriniz"></textarea><br />

    <button class="btn btn-info">Kaydet</button>

</form>